<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="ReverseProxyToFlask" stopProcessing="true">
                    <match url="(.*)" />
                    <conditions logicalGrouping="MatchAll" />
                    <action type="Rewrite" url="http://127.0.0.1:5000/{R:1}" logRewrittenUrl="true" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
